<h1 class="end_user_index">会員一覧</h1>

<div class="end_user_container">
  <% @end_users.each do |end_user| %>
  <table class="end_user_table">
    <tr>
      <th>氏名</th>
      <th>会員タイプ</th>
      <th>退会ステータス</th>
    </tr>
    <tr>
      <td><%= link_to end_user.name, admin_end_user_path(end_user.id) %></td>
      <td><%= end_user.end_user_status %></td>
      <td><%= end_user.is_active %></td>
    </tr>
  </table>
  <% end %>
</div>