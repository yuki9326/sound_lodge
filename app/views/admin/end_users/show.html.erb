<div class="container">
  <div class="row">
    <h1 class="end_user_heading">会員情報詳細</h1>
  </div>

  <table class="table">
    <tr>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>会員タイプ</th>
      <th>プロフィール</th>
      <th>退会ステータス</th>
    </tr>
    <tbody>
      <tr>
        <td><%= @end_user.name %></td>
        <td><%= @end_user.email %></td>
        <td><%= @end_user.end_user_status %></td>
        <td><% if @end_user.musician? %>
        <%= link_to "プロフィール", musician_profile_path(@end_user.musician_profile.id) %>
        <% end %></td>
        <td><%= @end_user.is_active %></td>
      </tr>
    </tbody>
  </table>
</div>